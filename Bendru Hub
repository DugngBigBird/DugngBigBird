			else
				if v:FindFirstChild('NameEsp'..Number) then
					v:FindFirstChild('NameEsp'..Number):Destroy()
				end
			end
		end   
	end)
end
end
StatsAESPTab:Toggle("ESP Player",false,function(a)
ESPPlayer = a
while ESPPlayer do wait()
	UpdatePlayerChams()
end
end)
StatsAESPTab:Toggle("ESP Chest",false,function(a)
ChestESP = a
while ChestESP do wait()
	UpdateChestChams() 
end
end)
StatsAESPTab:Toggle("ESP Devil Fruit",false,function(a)
DevilFruitESP = a
while DevilFruitESP do wait()
	UpdateDevilChams() 
end
end)
StatsAESPTab:Toggle("ESP Flower",false,function(a)
FlowerESP = a
while FlowerESP do wait()
	UpdateFlowerChams() 
end
end)

BendruHub:Init()
